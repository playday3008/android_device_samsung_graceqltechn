# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK


# mount_point		fstype	device						device2						flags
/boot			emmc	/dev/block/bootdevice/by-name/boot
/cache			ext4	/dev/block/bootdevice/by-name/cache
/data			ext4	/dev/block/bootdevice/by-name/userdata								flags=encryptable=footer;length=-16384
/persist		ext4	/dev/block/bootdevice/by-name/persist					    				flags=backup=1;display="Persist"
/recovery		emmc	/dev/block/bootdevice/by-name/recovery								flags=backup=1
/system		ext4	/dev/block/bootdevice/by-name/system
/system_image		emmc	/dev/block/bootdevice/by-name/system

/frp			emmc	/dev/block/bootdevice/by-name/frp									flags=display="FRP";backup=1
/modem			emmc	/dev/block/bootdevice/by-name/apnhlos									flags=display="apnhlos"

/firmware		vfat	/dev/block/bootdevice/by-name/apnhlos									flags=display="Firmware";fsflags=ro;mounttodecrypt
/firmware-modem	vfat	/dev/block/bootdevice/by-name/modem									flags=display="Firmware Modem";fsflags=ro

/misc			emmc	/dev/block/bootdevice/by-name/misc

# Bootloader
/aboot			emmc	/dev/block/bootdevice/by-name/aboot									flags=backup=1;display="Bootloader"
/cmnlib64		emmc	/dev/block/bootdevice/by-name/cmnlib64								flags=backup=1;subpartitionof=/aboot
/cmnlib		emmc	/dev/block/bootdevice/by-name/cmnlib									flags=backup=1;subpartitionof=/aboot
/hyp			emmc	/dev/block/bootdevice/by-name/hyp									flags=backup=1;subpartitionof=/aboot
/pmic			emmc	/dev/block/bootdevice/by-name/pmic									flags=backup=1;subpartitionof=/aboot
/tz			emmc	/dev/block/bootdevice/by-name/tz									flags=backup=1;subpartitionof=/aboot
/rpm			emmc	/dev/block/bootdevice/by-name/rpm									flags=backup=1;subpartitionof=/aboot
/keymaster		emmc	/dev/block/bootdevice/by-name/keymaster								flags=backup=1;subpartitionof=/aboot
/devcfg		emmc	/dev/block/bootdevice/by-name/devcfg									flags=backup=1;subpartitionof=/aboot
/xbl			emmc	/dev/block/bootdevice/by-name/xbl									flags=backup=1;subpartitionof=/aboot

# Modem/Bluetooth backup/restore
/mdm			emmc	/dev/block/bootdevice/by-name/modem									flags=backup=1;display="Modem"
/bluetooth		emmc	/dev/block/bootdevice/by-name/bluetooth								flags=backup=1;subpartitionof=/mdm
/dsp			emmc	/dev/block/bootdevice/by-name/dsp									flags=backup=1;subpartitionof=/mdm

# EFS backup/restore
/efs			ext4	/dev/block/bootdevice/by-name/efs									flags=backup=1;display="EFS"
/efs1			emmc	/dev/block/bootdevice/by-name/modemst1								flags=backup=1;subpartitionof=/efs
/efs2			emmc	/dev/block/bootdevice/by-name/modemst2								flags=backup=1;subpartitionof=/efs
/efsg			emmc	/dev/block/bootdevice/by-name/fsg									flags=backup=1;subpartitionof=/efs
/efsc			emmc	/dev/block/bootdevice/by-name/fsc									flags=backup=1;subpartitionof=/efs

# Removable storage
/external_sd		auto	/dev/block/mmcblk0p1 				/dev/block/mmcblk0					flags=display="Micro SD card";storage;wipeingui;removable
/usb-otg		auto	/dev/block/sde1 				/dev/block/sde						flags=display="USB-OTG";storage;wipeingui;removable
